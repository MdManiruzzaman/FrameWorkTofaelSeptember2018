<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="HM API Automation Script " parallel="tests">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="MacOS"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="60.0"/>
    <parameter name="url" value="https://www2.hm.com/en_us/"/>

    <test name = "Search ">
        <classes>
            <class name="homePageObjectTest.HomePageTest"/>
            <class name="homePageObjectTest.KeyWordLogInTest"/>
            <class name="homePageObjectTest.LogInByXlsTest"/>

        </classes>
    </test>

</suite>